{
    "openapi": "3.1.0",
    "info": {
        "title": "Twilio Number Search API",
        "description": "Simplified API for searching Twilio phone numbers and querying regulatory compliance",
        "version": "1.0.0"
    },
    "paths": {
        "/api/v1/sync": {
            "post": {
                "tags": [
                    "sync"
                ],
                "summary": "Trigger Sync",
                "description": "Trigger a sync of Twilio number types.\n\nNote: Twilio credentials are loaded from environment variables.\nRequest body can be empty.\n\nReturns:\n    Job ID for tracking sync status",
                "operationId": "trigger_sync_api_v1_sync_post",
                "responses": {
                    "202": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "additionalProperties": {
                                        "type": "string"
                                    },
                                    "title": "Response Trigger Sync Api V1 Sync Post"
                                }
                            }
                        }
                    }
                }
            },
            "get": {
                "tags": [
                    "sync"
                ],
                "summary": "List Sync Jobs",
                "description": "List recent sync jobs.\n\nArgs:\n    limit: Maximum number of jobs to return\n    \nReturns:\n    List of sync job statuses",
                "operationId": "list_sync_jobs_api_v1_sync_get",
                "parameters": [
                    {
                        "name": "limit",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 10,
                            "title": "Limit"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "additionalProperties": {
                                        "type": "array",
                                        "items": {}
                                    },
                                    "title": "Response List Sync Jobs Api V1 Sync Get"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/sync/{job_id}": {
            "get": {
                "tags": [
                    "sync"
                ],
                "summary": "Get Sync Status",
                "description": "Get status of a sync job.\n\nArgs:\n    job_id: Job ID returned from trigger_sync\n    \nReturns:\n    Sync job status",
                "operationId": "get_sync_status_api_v1_sync__job_id__get",
                "parameters": [
                    {
                        "name": "job_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Job Id"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/SyncStatus"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/sync/regulations": {
            "post": {
                "tags": [
                    "sync"
                ],
                "summary": "Trigger Regulations Sync",
                "description": "Trigger a sync of Twilio regulatory compliance regulations.\n\nNote: Twilio credentials are loaded from environment variables.\nRequest body can be empty.\n\nReturns:\n    Job ID for tracking sync status",
                "operationId": "trigger_regulations_sync_api_v1_sync_regulations_post",
                "responses": {
                    "202": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "additionalProperties": {
                                        "type": "string"
                                    },
                                    "type": "object",
                                    "title": "Response Trigger Regulations Sync Api V1 Sync Regulations Post"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/countries": {
            "get": {
                "tags": [
                    "countries"
                ],
                "summary": "List Countries",
                "description": "List all countries with available number types.\n\nArgs:\n    skip: Pagination offset\n    limit: Maximum number of results\n    number_type: Optional filter by number type availability\n    db: Database session\n    \nReturns:\n    List of country number types",
                "operationId": "list_countries_api_v1_countries_get",
                "parameters": [
                    {
                        "name": "skip",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "minimum": 0,
                            "description": "Number of records to skip",
                            "default": 0,
                            "title": "Skip"
                        },
                        "description": "Number of records to skip"
                    },
                    {
                        "name": "limit",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "maximum": 1000,
                            "minimum": 1,
                            "description": "Maximum number of records to return",
                            "default": 100,
                            "title": "Limit"
                        },
                        "description": "Maximum number of records to return"
                    },
                    {
                        "name": "number_type",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ],
                            "description": "Filter by number type (local, toll_free, mobile, etc.)",
                            "title": "Number Type"
                        },
                        "description": "Filter by number type (local, toll_free, mobile, etc.)"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/CountryNumberTypesResponse"
                                    },
                                    "title": "Response List Countries Api V1 Countries Get"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/countries/{country_code}": {
            "get": {
                "tags": [
                    "countries"
                ],
                "summary": "Get Country",
                "description": "Get number types for a specific country.\n\nArgs:\n    country_code: ISO country code (e.g., \"US\")\n    db: Database session\n    \nReturns:\n    Country number types",
                "operationId": "get_country_api_v1_countries__country_code__get",
                "parameters": [
                    {
                        "name": "country_code",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Country Code"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CountryNumberTypesResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/regulations/{country_code}": {
            "get": {
                "tags": [
                    "regulations"
                ],
                "summary": "Get Regulations",
                "description": "Get regulatory requirements for a specific country.\nAlways returns regulations with end_user_type='business'.\nQueries database only - data must be synced first using POST /api/v1/sync/regulations.\n\nBy default, returns all regulations regardless of number type availability, as some\nnumber types are available through private offerings and not exposed in the public API.\n\nArgs:\n    country_code: ISO country code (e.g., \"US\")\n    number_type: Optional filter by number type\n    only_available_types: Only show regulations for phone types available in the country (default: False)\n    db: Database session\n    \nReturns:\n    List of regulations for the specified country\n    \nRaises:\n    404: If no regulations found for the country",
                "operationId": "get_regulations_api_v1_regulations__country_code__get",
                "parameters": [
                    {
                        "name": "country_code",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Country Code"
                        }
                    },
                    {
                        "name": "number_type",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ],
                            "description": "Filter by number type (local, toll_free, mobile, etc.)",
                            "title": "Number Type"
                        },
                        "description": "Filter by number type (local, toll_free, mobile, etc.)"
                    },
                    {
                        "name": "only_available_types",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "boolean",
                            "description": "Only show regulations for available phone types (default: False to show all regulations including private offerings)",
                            "default": false,
                            "title": "Only Available Types"
                        },
                        "description": "Only show regulations for available phone types (default: False to show all regulations including private offerings)"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/RegulationResponse"
                                    },
                                    "title": "Response Get Regulations Api V1 Regulations  Country Code  Get"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/regulations/{regulation_sid}/export": {
            "get": {
                "tags": [
                    "regulations"
                ],
                "summary": "Export Regulation",
                "description": "Export a single regulation as an Excel template/checklist.\n\nArgs:\n    regulation_sid: The SID of the regulation to export\n    db: Database session\n    \nReturns:\n    Excel file download",
                "operationId": "export_regulation_api_v1_regulations__regulation_sid__export_get",
                "parameters": [
                    {
                        "name": "regulation_sid",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Regulation Sid"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/regulations/export": {
            "post": {
                "tags": [
                    "regulations"
                ],
                "summary": "Export Regulations",
                "description": "Export multiple regulations as a multi-tab Excel file.\n\nArgs:\n    request: Request body containing list of regulation SIDs\n    db: Database session\n    \nReturns:\n    Excel file download with one tab per regulation",
                "operationId": "export_regulations_api_v1_regulations_export_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ExportRegulationsRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/numbers/search": {
            "post": {
                "tags": [
                    "numbers"
                ],
                "summary": "Search Numbers",
                "description": "Search for available phone numbers by country and number type.\n\nNote: When both sms_enabled and voice_enabled are specified, filters are applied with AND logic\n(numbers must match both criteria). If no results are returned, try searching with only one filter\nor without filters to see available numbers.\n\nNote: Twilio credentials are loaded from environment variables.\n\nArgs:\n    request: Search request containing country_code, number_type, and optional filters\n    \nReturns:\n    List of available phone numbers matching the criteria\n    \nRaises:\n    400: If invalid number type or request parameters\n    500: If Twilio API error occurs or credentials are not configured",
                "operationId": "search_numbers_api_v1_numbers_search_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/NumberSearchRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": {
                                        "$ref": "#/components/schemas/AvailableNumberResponse"
                                    },
                                    "type": "array",
                                    "title": "Response Search Numbers Api V1 Numbers Search Post"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/": {
            "get": {
                "summary": "Root",
                "description": "Root endpoint.",
                "operationId": "root__get",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        },
        "/health": {
            "get": {
                "summary": "Health Check",
                "description": "Health check endpoint.",
                "operationId": "health_check_health_get",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    }
                }
            }
        }
    },
